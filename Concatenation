#include <iostream>
#include <string>
using namespace std;
int main() {
    string firstName, secondname, fullname;
    cout << "Enter your first name: ";
    cin >> firstname;
    cout << "Enter your second name: ";
    cin >> secondname;
    fullName = firstname + " " + secondname;
    cout << "Full Name: " << fullname << endl;
    return 0;
}


OUTPUT
Enter your first name: Anavi
Enter your second name: Kashyup
Full Name: Anavi Kashyup
